<h2>Listing Recyclings</h2>

<table class="table">
  <thead>
    <tr>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for samples <- @recyclings do %>
    <tr>

      <td class="text-right">
        <span><%= link "Show", to: recycling_samples_path(@conn, :show, samples), class: "btn btn-default btn-xs" %></span>
        <span><%= link "Edit", to: recycling_samples_path(@conn, :edit, samples), class: "btn btn-default btn-xs" %></span>
        <span><%= link "Delete", to: recycling_samples_path(@conn, :delete, samples), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Samples", to: recycling_samples_path(@conn, :new) %></span>
